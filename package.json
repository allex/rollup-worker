{
  "name": "rollup-worker",
  "version": "2.0.1",
  "description": "Rollup worker for multiple entry bundle with customize distributes.",
  "main": "lib/rollup-worker.js",
  "module": "lib/rollup-worker.es.js",
  "repository": "allex/rollup-worker",
  "keywords": [
    "rollup",
    "rollup-worker",
    "rb",
    "bundle",
    "zero-config"
  ],
  "bin": {
    "rb": "./bin/cli.js",
    "rollup-bundle": "./bin/cli.js",
    "rollup-worker": "./bin/cli.js"
  },
  "scripts": {
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:ts": "tslint --project .",
    "lint:js": "standard \"test/*.js\"",
    "lint:fix": "tslint --project . --fix && standard \"src/*.js\" --fix",
    "build": "rb -c && chmod a+x bin/cli.js",
    "watch": "export NODE_ENV='development' && rb -c -w",
    "test": "npm run lint"
  },
  "author": "allex <allex.wxn@gmail.com> (http://iallex.com/)",
  "license": "MIT",
  "dependencies": {
    "@allex/md5": "^1.0.4",
    "@allex/rollup-plugin-node-globals": "^0.0.2",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@rollup/plugin-babel": "^5",
    "@rollup/plugin-commonjs": "^19",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13",
    "@rollup/plugin-replace": "^2",
    "@rollup/plugin-typescript": "^11",
    "autoprefixer": "^9",
    "babel-plugin-macros": "^2.7.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "babel-plugin-transform-replace-expressions": "^0.2.0",
    "colorette": "^2.0.20",
    "cssnano": "^4",
    "debug": "^3.1.0",
    "postcss": "^8.x",
    "resolve-from": "^5.0.0",
    "rollup": "^2",
    "rollup-plugin-json5": "^1",
    "rollup-plugin-jspacker": "^1",
    "rollup-plugin-node-builtins": "^2",
    "rollup-plugin-postcss": "^4",
    "rollup-plugin-progress": "^1"
  },
  "devDependencies": {
    "@fdio/utils": "^1",
    "ansi-escapes": "^3.1.0",
    "babel-plugin-alter-object-assign": "^1.0.2",
    "date-time": "^2.1.0",
    "mkdirp": "^0.5.1",
    "pretty-bytes": "^5.1.0",
    "pretty-ms": "^3.1.0",
    "signal-exit": "^3.0.2",
    "tslib": "^2",
    "tslint": "^5.12.1"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "lib",
    "package.json"
  ],
  "files": [
    "lib",
    "bin"
  ],
  "engines": {
    "node": ">=7.10.1"
  }
}
