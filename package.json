{
  "name": "rollup-worker",
  "version": "1.0.7",
  "description": "Rollup worker for multiple entry bundle with customize distributes.",
  "main": "lib/rollup-worker.js",
  "module": "lib/rollup-worker.es.js",
  "repository": "allex/rollup-worker",
  "keywords": [
    "rollup",
    "bundle",
    "multiple"
  ],
  "bin": {
    "rollup-bundle": "./bin/cli.js",
    "rollup-worker": "./bin/cli.js"
  },
  "scripts": {
    "lint": "npm run lint:ts -- --fix && npm run lint:js -- --fix",
    "lint:ts": "tslint --project .",
    "lint:js": "standard \"test/*.js\"",
    "lint:fix": "standard \"src/*.js\" --fix",
    "build": "rollup -c && shx chmod a+x bin/cli.js",
    "watch": "rollup -c -w",
    "test": "npm run lint",
    "prepare": "npm test && npm run build"
  },
  "author": "allex <allex.wxn@gmail.com> (http://iallex.com/)",
  "license": "MIT",
  "dependencies": {
    "@allex/rollup-plugin-node-resolve": "next",
    "babel-core": "6",
    "chalk": "^2.4.1",
    "debug": "^3.1.0",
    "rollup": "~1.1.2",
    "rollup-plugin-babel": "^3",
    "rollup-plugin-commonjs": "^9",
    "rollup-plugin-json5": "^1",
    "rollup-plugin-progress": "^1.0.0",
    "rollup-plugin-replace": "^2",
    "uglify-js": "3.4.1"
  },
  "devDependencies": {
    "ansi-escapes": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "date-time": "^2.1.0",
    "fast-async": "^6",
    "mkdirp": "^0.5.1",
    "nodent-runtime": "^3.2.0",
    "pretty-bytes": "^5.1.0",
    "pretty-ms": "^3.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "shx": "^0.2.2",
    "signal-exit": "^3.0.2",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1"
  },
  "files": [
    "lib",
    "bin"
  ],
  "engines": {
    "node": ">=7.10.1"
  }
}
