{
  "name": "rollup-worker",
  "version": "1.0.6-beta.1",
  "description": "Rollup worker for multiple entry bundle with customize distributes.",
  "main": "lib/rollup-worker.js",
  "module": "lib/rollup-worker.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/allex/rollup-worker.git"
  },
  "keywords": [
    "rollup",
    "bundle",
    "multiple"
  ],
  "bin": {
    "rollup-bundle": "./bin/cli.js",
    "rollup-worker": "./bin/cli.js"
  },
  "scripts": {
    "lint": "standard \"src/*.js\"",
    "lint:fix": "standard \"src/*.js\" --fix",
    "build": "rollup -c && shx chmod a+x bin/cli.js",
    "watch": "rollup -w -c",
    "test": "npm run lint",
    "prepare": "npm run build"
  },
  "author": "allex <allex.wxn@gmail.com> (http://iallex.com/)",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "debug": "^3.1.0",
    "rollup": "^0.59.0",
    "rollup-plugin-babel": "^3",
    "rollup-plugin-commonjs": "^9",
    "rollup-plugin-node-resolve": "^3",
    "uglify-js": "^3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "fast-async": "^6",
    "nodent-runtime": "^3.2.0",
    "shx": "^0.2.2",
    "date-time": "^2.1.0",
    "mkdirp": "^0.5.1",
    "pretty-ms": "^3.1.0",
    "ansi-escapes": "^3.1.0",
    "signal-exit": "^3.0.2"
  },
  "files": [
    "lib",
    "bin"
  ],
  "engines": {
    "node": ">=5.12.0"
  }
}
