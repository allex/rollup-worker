{
  "presets": [
    ["env", {
      "loose": true,
      "modules": false,
      "useBuiltIns": false,
      "exclude": ["transform-regenerator", "transform-async-to-generator"],
      "targets": {
        "node": "7.10.1"
      }
    }]
  ],
  "plugins": [
    [ "transform-class-properties", { "spec": true } ],
    [ "transform-object-rest-spread", { "useBuiltIns": true } ]
  ],
  "comments": false
}
